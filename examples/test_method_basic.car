programa "MethodCallBasic" {
    // Basic array operations
    var numbers: [inteiro32] = [10, 20, 30];
    
    // Get array length
    var size: inteiro32 = numbers.len;
    escreval("Array size: ${size}");
    
    // Add elements
    numbers.push(40);
    numbers.push(50);
    escreval("After push, size: ${numbers.len}");
    
    // Remove last element
    numbers.pop();
    escreval("After pop, size: ${numbers.len}");
}

