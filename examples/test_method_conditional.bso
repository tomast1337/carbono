programa "MethodCallConditional" {
    var data: [inteiro32] = [1, 2, 3];
    
    // Get length first
    var len: inteiro32 = data.len;
    
    // Conditional push
    se (len < 5) {
        data.push(4);
        data.push(5);
    }
    
    escreval("Final array size: ${data.len}");
    
    // Conditional pop - get length in variable
    var size: inteiro32 = data.len;
    enquanto (size > 0) {
        escreval("Popping, remaining: ${size}");
        data.pop();
        size = data.len;
    }
    
    escreval("Final size: ${data.len}");
}

