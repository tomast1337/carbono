programa "MethodCallComplex" {
    // Complex example combining multiple features
    var stack: [inteiro32] = [];
    
    escreval("Building a stack...");
    
    // Push multiple values
    cada (i: 1..6) {
        stack.push(i * 10);
        escreval("Pushed ${i * 10}, stack size: ${stack.len}");
    }
    
    escreval("Final stack size: ${stack.len}");
    
    // Process stack
    enquanto (stack.len > 0) {
        var top: inteiro32 = stack[stack.len - 1];
        escreval("Top element: ${top}, remaining: ${stack.len}");
        stack.pop();
    }
    
    escreval("Stack size: ${stack.len}");
    
    // Rebuild with different pattern
    var evens: [inteiro32] = [];
    cada (i: 0..10) {
        se (i % 2 == 0) {
            evens.push(i);
        }
    }
    
    escreval("Even numbers count: ${evens.len}");
}

